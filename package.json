{
    "name": "eslib-stack",
    "version": "v8.0.0",
    "description": "A small & flexible stack for building modern ES6 libraries.",
    "main": "dist/myLib.common.js",
    "module": "dist/myLib.esm.js",
    "constants": {
        "dirBase": {
            "input": "./src",
            "output": "./dist"
        },
        "js": {
            "input": "main",
            "namespace": {
                "module": "myLib",
                "file": "myLib"
            },
            "targets": {
                "browsers": "chrome >= 58"
            }
        }
    },
    "scripts": {
        "start": "npm-run-all lint build test",
        "build": "npm-run-all dev",
        "dev": "node tasks/jsDev.js",
        "dist": "node tasks/jsDist.js",
        "lint": "eslint src/",
        "test": "jasmine"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FelixRilling/eslib-stack.git"
    },
    "keywords": [
        "stack",
        "es"
    ],
    "author": "Felix Rilling",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/FelixRilling/eslib-stack/issues"
    },
    "homepage": "https://github.com/FelixRilling/eslib-stack#readme",
    "devDependencies": {
        "babel-core": "^6.26.0",
        "babel-plugin-external-helpers": "^6.22.0",
        "babel-preset-env": "^1.6.1",
        "eslint": "^4.10.0",
        "jasmine": "^2.8.0",
        "npm-run-all": "^4.1.1",
        "rollup": "^0.50.0",
        "rollup-plugin-babel": "^3.0.2",
        "rollup-plugin-node-resolve": "^3.0.0",
        "rollup-plugin-uglify-es": "0.0.1",
        "uglify-es": "^3.1.6"
    },
    "dependencies": {}
}
