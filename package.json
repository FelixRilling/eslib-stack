{
    "name": "eslib-stack",
    "version": "v2.0.0",
    "description": "A small & flexible stack for building modern ES6 libraries.",
    "main": "dist/my-lib.js",
    "module": {
        "name": "myLib",
        "id": "my-lib"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FelixRilling/eslib-stack.git"
    },
    "scripts": {
        "start": "npm run dist",
        "dist": "npm run sync  && npm run build && npm run uglify && npm run header",
        "build": "npm run bundle && npm run babel",
        "bundle": "rollup -c",
        "babel": "rollup -c ./rollup.babel.config.js",
        "uglify": "node ./tasks/uglify.js",
        "sync": "node ./tasks/sync.js",
        "header": "node ./tasks/header.js"
    },
    "keywords": [
        "dependencies",
        "services"
    ],
    "author": "Felix Rilling",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/FelixRilling/eslib-stack/issues"
    },
    "homepage": "https://github.com/FelixRilling/eslib-stack#readme",
    "devDependencies": {
        "babel-plugin-external-helpers": "^6.8.0",
        "babel-preset-es2015": "^6.14.0",
        "rollup": "^0.34.0",
        "rollup-plugin-babel": "^2.6.1",
        "rollup-plugin-commonjs": "^3.3.0",
        "rollup-plugin-node-resolve": "^2.0.0",
        "uglify-js": "^2.7.0"
    },
    "dependencies": {}
}
