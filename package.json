{
    "name": "eslib-stack",
    "version": "v2.8.0",
    "description": "A small & flexible stack for building modern ES6 libraries.",
    "main": "dist/myLib.common.js",
    "namespace": {
        "module": "myLib",
        "file": "myLib"
    },
    "scripts": {
        "start": "npm run lint && npm run build && npm run test",
        "build": "node build.js",
        "lint": "./node_modules/.bin/eslint ./src/",
        "test": "./node_modules/.bin/jasmine"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FelixRilling/eslib-stack.git"
    },
    "keywords": [
        "stack",
        "es"
    ],
    "author": "Felix Rilling",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/FelixRilling/eslib-stack/issues"
    },
    "homepage": "https://github.com/FelixRilling/eslib-stack#readme",
    "devDependencies": {
        "babel-core": "^6.24.1",
        "babel-preset-es2016": "^6.24.1",
        "eslint": "^3.19.0",
        "jasmine": "^2.5.3",
        "rollup": "^0.41.6",
        "uglify-js": "git://github.com/mishoo/UglifyJS2.git#harmony"
    }
}
