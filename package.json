{
    "name": "eslib-stack",
    "version": "v6.0.0",
    "description": "A small & flexible stack for building modern ES6 libraries.",
    "main": "dist/myLib.common.js",
    "module": "dist/myLib.esm.js",
    "constants": {
        "dirBase": {
            "input": "./src",
            "output": "./dist"
        },
        "js": {
            "input": "main",
            "namespace": {
                "module": "myLib",
                "file": "myLib"
            }
        }
    },
    "scripts": {
        "start": "npm-run-all lint build test",
        "build": "node tasks/jsDev.js",
        "dist": "node tasks/jsDist.js",
        "lint": "eslint src/",
        "test": "jasmine"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FelixRilling/eslib-stack.git"
    },
    "keywords": [
        "stack",
        "es"
    ],
    "author": "Felix Rilling",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/FelixRilling/eslib-stack/issues"
    },
    "homepage": "https://github.com/FelixRilling/eslib-stack#readme",
    "devDependencies": {
        "babel-core": "^7.0.0-alpha.19",
        "babel-preset-env": "^1.6.0",
        "eslint": "^4.5.0",
        "eslint-plugin-unicorn": "^2.1.2",
        "jasmine": "^2.8.0",
        "npm-run-all": "^4.1.0",
        "rollup": "^0.48.2",
        "uglify-es": "^3.0.28"
    },
    "dependencies": {}
}
